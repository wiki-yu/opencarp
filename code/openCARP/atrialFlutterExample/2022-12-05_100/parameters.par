## atrialFlutter.par ##
# ionic model setup
num_imp_regions = 1 
imp_region[0].im = Nygren	# select ionic model
# simulation setup
bidomain    =   1 	# run bidomain
spacedt     =   10.	# output every 0.01 ms
timedt      =   10.	# update progess on commandline every ms
parab_solve =   1	# use Crank-Nicolson to solve the parabolic problem
cg_maxit_ellip = 1000

##END of atrialFlutter.par
#=======================================================<<

